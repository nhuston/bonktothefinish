<% data.tags.sort_by { |k, v| v[:ranking] }.each do |tag_name,tag_info|
  is_icon = !tag_info.key?(:img_url)
  img_url = tag_info.img_url || tag_info.icon_url
  %>
  <%= partial "partials/card.erb", locals: {
      articles:blog.tags[tag_name],
      img_url: img_url,
      is_icon: is_icon,
      tag_name: tag_name } %>
<% end %>
